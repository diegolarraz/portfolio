<div class="contact-me" id="contact-me">
  <div class="container reveal">
    <div class="row">
      <div class="hiddex-xs col-sm-2 col-md-3 col-lg-3"></div>
      <div class="col-xs-12 col-sm-8 col-md-6 col-lg-6 text-center">
        <span class="title">Contact me</span><br><br>
        <% if flash[:notice] %>
            <div class="alert alert-success"><%= flash[:notice] %></div>
          <% elsif flash[:warning] %>
            <div class="alert alert-danger"><%= flash[:warning] %></div>
          <% else %>
        <% end %>
        <%= form_for :contact do |f| %>
          <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
              <input class="form-control" type="text" id="fname" name="contact[name]" placeholder="Name:">
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
              <input class="form-control" type="text" id="lname" name="contact[email]" placeholder="Email:">
            </div>
          </div>
          <br>
          <span><textarea class="form-control" id="subject" name="contact[message]" placeholder="Message:" style="height:100px"></textarea></span>
          <br>
          <input class="btn" type="submit" value="Submit">
        <% end %>
        <br><br><br>
        <div class="line"></div><br>
        <a href="https://www.linkedin.com/in/shouhei-yamauchi/" target="_blank"><i class="fab fa-linkedin"></i></a>&nbsp;
        <a href="https://github.com/diegolarraz" target="_blank"><i class="fab fa-github-square"></i></a>&nbsp;
        <br><br>
        <span class="copyright">&copy; 2020 Diego Larraz</span>
      </div>
      <div class="hiddex-xs col-sm-2 col-md-3 col-lg-3"></div>
    </div>
  </div>
</div>

